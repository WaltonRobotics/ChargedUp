public class LEDSubsystem extends GamePieceMode
{
    public void setColor(GamePieceMode)
    {
        if(GamePieceMode == CONE) {
            led = yellow;
        }
        
        if(GamePieceMode == CUBE) {
            led = purple;
        }


    }
}
